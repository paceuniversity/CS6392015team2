<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.litianyu.automobilemaintenance" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".setup"
            android:label="@string/title_activity_setup" >
        </activity>
        <activity
            android:name=".automain"
            android:label="@string/title_activity_automain" >
        </activity>
        <activity
            android:name=".remainder"
            android:label="@string/title_activity_remainder" >
        </activity>
        <activity
            android:name=".quickdial"
            android:label="@string/title_activity_quickdial" >
        </activity>
    </application>

</manifest>
